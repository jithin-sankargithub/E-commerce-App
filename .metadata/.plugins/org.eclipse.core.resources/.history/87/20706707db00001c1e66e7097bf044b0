package com.shopping.userservice.service;

import java.util.List;
import java.util.Optional;

import com.shopping.userservice.model.UserProfile;

public interface UserProfileService {
	
	public UserProfile addNewCustomerProfile(UserProfile userProfile);
	public Optional<UserProfile> getByProfileId(String profileId);
	public UserProfile updateProfile(UserProfile userProfile);
	public UserProfile getByUserName(String name);
	public UserProfile addNewMerchantProfile(UserProfile userprofile);
	public String deleteUser(String profileId);
	public List<UserProfile> viewAllUsers();

}
